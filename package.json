{
  "name": "n8n-nodes-honeycomb",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Honeycomb observability platform providing 12 resources and 50+ operations for querying telemetry data, managing datasets, boards, triggers, SLOs, and events.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-honeycomb.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-honeycomb#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-honeycomb/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "honeycomb",
    "observability",
    "monitoring",
    "telemetry",
    "tracing",
    "apm",
    "slo",
    "alerts",
    "triggers",
    "metrics",
    "events",
    "debugging"
  ],
  "main": "dist/nodes/Honeycomb/Honeycomb.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/HoneycombApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Honeycomb/Honeycomb.node.js",
      "dist/nodes/Honeycomb/HoneycombTrigger.node.js"
    ]
  },
  "files": [
    "dist",
    "LICENSE",
    "COMMERCIAL_LICENSE.md",
    "LICENSING_FAQ.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.30.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
